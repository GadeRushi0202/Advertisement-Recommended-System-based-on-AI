        <servlet-name>SendRequest</servlet-name>
        <url-pattern>/SendRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>imageServlet</servlet-name>
        <url-pattern>/imageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikePage</servlet-name>
        <url-pattern>/LikePage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Forgot</servlet-name>
        <url-pattern>/Forgot</url-pattern>
    </servlet-mapping>
