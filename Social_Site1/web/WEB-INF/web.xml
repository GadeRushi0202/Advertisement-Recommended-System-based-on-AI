        <servlet-name>DislikePage</servlet-name>
        <url-pattern>/DislikePage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikeItems</servlet-name>
        <url-pattern>/LikeItems</url-pattern>
    </servlet-mapping>
