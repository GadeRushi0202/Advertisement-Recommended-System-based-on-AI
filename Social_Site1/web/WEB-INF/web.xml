<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CreatecardCheck</servlet-name>
        <servlet-class>action.CreatecardCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>action.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostMessage</servlet-name>
        <servlet-class>action.PostMessage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>action.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendRequest</servlet-name>
        <servlet-class>action.SendRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>imageServlet</servlet-name>
        <servlet-class>action.imageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikePage</servlet-name>
        <servlet-class>action.LikePage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Forgot</servlet-name>
        <servlet-class>action.Forgot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DislikePage</servlet-name>
        <servlet-class>action.DislikePage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikeItems</servlet-name>
        <servlet-class>action.LikeItems</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreatecardCheck</servlet-name>
        <url-pattern>/CreatecardCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostMessage</servlet-name>
        <url-pattern>/PostMessage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
